{
  "layer": "top",
  "position": "bottom",
  "height": 40,

  "modules-left": ["wlr/taskbar"],
  "modules-center": [],
  "modules-right": ["tray", "clock"],

  "tray": {
    "spacing": 5,
  },

  "wlr/taskbar": {
    "format": "{icon}   {title:.25}",
    "markup": true,
    "icon-size": 20,
    "on-click": "activate",
  },

  "clock": {
    "format": "{:>%I:%M:%S %p \n %m/%d/%Y}",
    "interval": 1,
    "align": 1,
    "justify": "right",
  },
}
